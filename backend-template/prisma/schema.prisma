// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id Int @id @default(autoincrement())
  name String
  building String
  location String
  start_time DateTime
  description String
  link String
  image_uri String
}